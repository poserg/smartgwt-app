<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1//EN"
    "http://google-web-toolkit.googlecode.com/svn/releases/2.1/distro-source/core/src/gwt-module.dtd">
    
<module>

      <!-- Inherit the core GWT stuff. -->
      <inherits name='com.google.gwt.user.User'/>

      <!-- Inherit the SmartGWT stuff. -->
      <inherits name='com.smartgwt.SmartGwt' />
      <inherits name="com.smartgwt.tools.SmartGwtTools"/>

      <!-- <inherits name="com.smartclient.theme.treefrog.TreeFrog" /> -->

      <!-- Limit builds to your preferred browser. -->

      <!-- Safari -->
      <!-- <set-property name="user.agent" value="safari"/> -->

      <!-- Firefox 3 -->
      <set-property name="user.agent" value="gecko1_8"/>

      <!-- Specify the app entry point class. -->
      <entry-point class='com.github.poserg.client.Application'/>

      <!-- Specify the application-specific style sheet. -->
      <stylesheet src='Application.css' />

</module>
    
